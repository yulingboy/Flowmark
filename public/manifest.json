{
  "manifest_version": 3,
  "name": "Flowmark",
  "version": "1.0.0",
  "description": "简洁高效的浏览器起始页",
  "icons": {
    "128": "icons/icon.svg"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://*/*",
    "http://*/*"
  ]
}
